   "source": [
    "from collections import Counter\n",
    "joke = \"A string with some words they contain letters\"\n",
    "j2 = []\n",
    "for w in joke.split():\n",
    "    d = Counter(w)\n",
    "    if 'e' in d.keys():\n",
    "        if d['e'] == 1:\n",
    "            j2.append(w)"
   ]
  },
